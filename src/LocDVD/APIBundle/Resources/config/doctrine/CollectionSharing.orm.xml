<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LocDVD\APIBundle\Entity\CollectionSharing" table="collection_sharing">
    <unique-constraints>
      <unique-constraint name="sharing_ukey" columns="collection_id"/>
    </unique-constraints>
    <id name="id" type="string" column="id" length="64">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="availDate" type="date" column="avail_date" nullable="false"/>
    <field name="expDate" type="date" column="exp_date" nullable="false"/>
    <field name="permanent" type="boolean" column="permanent" nullable="false"/>
    <many-to-one field="collection" target-entity="Collection">
      <join-columns>
        <join-column name="collection_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
