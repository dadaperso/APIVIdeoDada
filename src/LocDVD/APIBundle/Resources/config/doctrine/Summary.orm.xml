<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LocDVD\APIBundle\Entity\Summary" table="summary">
    <unique-constraints>
      <unique-constraint name="summary_umapper" columns="mapper_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="summary" type="text" column="summary" nullable="false"/>
    <field name="createDate" type="datetime" column="create_date" nullable="true"/>
    <field name="modifyDate" type="datetime" column="modify_date" nullable="true"/>
    <many-to-one field="mapper" target-entity="Mapper">
      <join-columns>
        <join-column name="mapper_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
