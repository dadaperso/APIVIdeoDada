<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LocDVD\APIBundle\Entity\WatchStatus" table="watch_status">
    <indexes>
      <index name="IDX_3C409980B9CA839A" columns="mapper_id"/>
      <index name="IDX_3C409980762690C1" columns="video_file_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="watch_status_ukey" columns="uid,video_file_id,mapper_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="uid" type="bigint" column="uid" nullable="false"/>
    <field name="position" type="integer" column="position" nullable="false"/>
    <field name="createDate" type="datetime" column="create_date" nullable="true"/>
    <field name="modifyDate" type="datetime" column="modify_date" nullable="true"/>
    <many-to-one field="videoFile" target-entity="VideoFile">
      <join-columns>
        <join-column name="video_file_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="mapper" target-entity="Mapper">
      <join-columns>
        <join-column name="mapper_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
