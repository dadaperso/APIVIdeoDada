<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LocDVD\APIBundle\Entity\Movie" table="movie">
    <indexes>
      <index name="movie_title_idx" columns="title"/>
      <index name="IDX_1D5EF26FFE2541D7" columns="library_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="movie_ukey" columns="library_id,title,year"/>
      <unique-constraint name="movie_umapper" columns="mapper_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="title" type="string" column="title" length="255" nullable="false"/>
    <field name="sortTitle" type="string" column="sort_title" length="255" nullable="false"/>
    <field name="tagLine" type="string" column="tag_line" length="255" nullable="false"/>
    <field name="year" type="integer" column="year" nullable="true"/>
    <field name="originallyAvailable" type="date" column="originally_available" nullable="true"/>
    <field name="sortTime" type="date" column="sort_time" nullable="true"/>
    <field name="islock" type="boolean" column="islock" nullable="true"/>
    <field name="createDate" type="timestamp" column="create_date" nullable="true"/>
    <field name="modifyDate" type="timestamp" column="modify_date" nullable="true"/>
    <field name="certificate" type="string" column="certificate" length="255" nullable="false"/>
    <many-to-one field="mapper" target-entity="Mapper">
      <join-columns>
        <join-column name="mapper_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="library" target-entity="Library">
      <join-columns>
        <join-column name="library_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
