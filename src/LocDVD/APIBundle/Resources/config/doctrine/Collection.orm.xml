<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LocDVD\APIBundle\Entity\Collection" table="collection">
    <indexes>
      <index name="collection_title_idx" columns="title"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="collection_ukey" columns="title,uid"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="uid" type="bigint" column="uid" nullable="false"/>
    <field name="title" type="string" column="title" length="255" nullable="false"/>
    <field name="createDate" type="datetime" column="create_date" nullable="true"/>
    <field name="modifyDate" type="datetime" column="modify_date" nullable="true"/>
    <field name="isSmart" type="boolean" column="is_smart" nullable="false"/>
  </entity>
</doctrine-mapping>
