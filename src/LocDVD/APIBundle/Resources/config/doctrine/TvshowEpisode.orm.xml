<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LocDVD\APIBundle\Entity\TvshowEpisode" table="tvshow_episode">
    <indexes>
      <index name="IDX_F9441196FE2541D7" columns="library_id"/>
      <index name="IDX_F94411966CD43D7A" columns="tvshow_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="tvshow_episode_umapper" columns="mapper_id"/>
      <unique-constraint name="tvshow_episode_ukey" columns="season,episode,tvshow_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="tagLine" type="string" column="tag_line" length="255" nullable="false"/>
    <field name="season" type="integer" column="season" nullable="true"/>
    <field name="episode" type="integer" column="episode" nullable="true"/>
    <field name="year" type="integer" column="year" nullable="true"/>
    <field name="originallyAvailable" type="date" column="originally_available" nullable="true"/>
    <field name="sortTime" type="date" column="sort_time" nullable="true"/>
    <field name="islock" type="boolean" column="islock" nullable="true"/>
    <field name="createDate" type="datetime" column="create_date" nullable="true"/>
    <field name="modifyDate" type="datetime" column="modify_date" nullable="true"/>
    <field name="certificate" type="string" column="certificate" length="255" nullable="false"/>
    <many-to-one field="tvshow" target-entity="Tvshow">
      <join-columns>
        <join-column name="tvshow_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="mapper" target-entity="Mapper">
      <join-columns>
        <join-column name="mapper_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="library" target-entity="Library">
      <join-columns>
        <join-column name="library_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
