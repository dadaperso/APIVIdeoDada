<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LocDVD\APIBundle\Entity\Actor" table="actor">
    <indexes>
      <index name="actor_idx" columns="actor"/>
      <index name="IDX_447556F9B9CA839A" columns="mapper_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="actor_mapper_ukey" columns="actor,mapper_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="actor" type="string" column="actor" length="255" nullable="false"/>
    <field name="createDate" type="datetime" column="create_date" nullable="true"/>
    <field name="modifyDate" type="datetime" column="modify_date" nullable="true"/>
    <many-to-one field="mapper" target-entity="Mapper">
      <join-columns>
        <join-column name="mapper_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
